<!DOCTYPE html>
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Safe-Journey
        link(rel="stylesheet", href="css/reset.css")
        link(rel="stylesheet", href="css/style.css")
        script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery/1.10.1/jquery.min.js')

    body
        .wrapper
            .row
                header.header.container
                    nav.nav
                        .logo
                            a(href="index.html")
                                img(src="img/logo.png")
                        ul.nav__list
                            li.nav__item
                                a(href="javascript:void(0)").nav__link.nav__link--active Home
                            li.nav__item
                                a(href="javascript:void(0)").nav__link About Us
                            li.nav__item
                                a(href="compare-prices.html").nav__link Compare Prices
                            li.nav__item
                                a(href="javascript:void(0)").nav__link Booking Status
            .row
                //- search
                .search.container
                    ul.search__list-step
                        li.search__step
                            a.search__link(href="#")
                                span.search__number 1
                                | Search For Bus
                        li.search__step
                            a.search__link.active(href="#")
                                span.search__number 2
                                | Select Bus
                        li.search__step
                            a.search__link(href="#")
                                span.search__number 3
                                | Select Bus Seat
                        li.search__step
                            a.search__link(href="#")
                                span.search__number 4
                                | Passengers Info
                        li.search__step
                            a.search__link(href="#")
                                span.search__number 5
                                | Payment
                    .box.search__box
                        .box__container.search__container
                            h1.search__title ONE WAY
                            .box__date.content1.flex
                                .input-box.no-margin
                                    label(for="park")
                                    span.icon3.icon--place
                                    input(type="text" placeholder="Search by State & Park")#park.input.input3.no-radius
                                .input-box.no-margin
                                    label(for="state")
                                    span.icon3.icon--place
                                    input(type="text" placeholder="To State")#state.input.input3.no-radius
                                .input-box.no-margin
                                    label(for="date")
                                    span.icon3
                                    input(type="text" placeholder="Departure Date" onfocus="(this.type='date')")#date.input.input3.no-radius
                                .input-box.no-margin
                                    label(for="seat")
                                    input(type="text" placeholder="No of Seat")#seat.input.input4.no-radius
                                .btn.btn--search.btn--white Search
                    .departure
                        .departure__item
                            p.departure__title Departure Time
                            .departure__box
                                .departure__box-item 00:00
                                .departure__box-item 24:00
                            .departure__range
                                .departure__range--box#rangeTime
                                    span.icon.icon--back-label
                                    label(for="time") 09:00
                                    span.icon.icon--next-label
                                input(type="range" id="time" class="range--custom" min="1" max="24" step="1" value="11")
                        .departure__item
                            p.departure__title Price Range
                            .departure__box
                                .departure__box-item ₦20,000
                                .departure__box-item ₦150,000
                            .departure__range
                                .departure__range--box#rangeMoney
                                    span.icon.icon--back-label
                                    label(for="money") ₦80,000
                                    span.icon.icon--next-label
                                input(type="range" id="money" class="range--custom" min="20" max="150" step="2" value="78")
                    .result
                        .result__top
                            h3.item__title Search Result
                            span.icon.icon--list
                        .result__list
                            .result__box
                                .result__item.result__img
                                    img(src="img/bus1.png", alt="")
                                .result__item.result__time
                                    .result__row
                                        p.row--title Departure
                                        p.row--title Arrival
                                    .result__row
                                        span.row--address.row--padding LAG
                                        span.row--address.row--flex
                                            span.icon.icon--arrival
                                            span.row--mini-time 1hr 20 mins
                                        span.row--address.row--padding EDO
                                    .result__row
                                        p.row--time.row--padding 16:00
                                        p.row--time.row--padding 09:45
                                    .result__row
                                        p.row--date.row--padding 12 Jan
                                        p.row--date.row--padding 13 Jan
                                .result__item.result__info
                                    ul.result__service
                                        li
                                            a(href="#") 14 Seaters Arrival
                                        li
                                            a(href="#") 4.3 Rating
                                        li
                                            a(href="#") Fare Rule
                                        li
                                            a(href="#") Amenities
                                .result__item.result__price
                                    .result__price-box
                                        p.result__price-title N80,000
                                        .result__button
                                            a(href="#").btn.btn--result.btn--details Details
                                            a(href="select-seat.html").btn.btn--result.btn--select Select
                            .result__box
                                .result__item.result__img
                                    img(src="img/bus2.png", alt="")
                                .result__item.result__time
                                    .result__row
                                        p.row--title Departure
                                        p.row--title Arrival
                                    .result__row
