                                        span.row--address.row--flex
                                            span.icon.icon--arrival
                                            span.row--mini-time 1hr 20 mins
                                        span.row--address.row--padding EDO
                                    .result__row
                                        p.row--time.row--padding 16:00
                                        p.row--time.row--padding 09:45
                                    .result__row
                                        p.row--date.row--padding 12 Jan
                                        p.row--date.row--padding 13 Jan
                                .result__item.result__info
                                    ul.result__service
                                        li
                                            a(href="#") 14 Seaters Arrival
                                        li
                                            a(href="#") 4.3 Rating
                                        li
                                            a(href="#") Fare Rule
                                        li
                                            a(href="#") Amenities
                                .result__item.result__price
                                    .result__price-box
                                        p.result__price-title N80,000
                                        .result__button
                                            a(href="#").btn.btn--result.btn--details Details
                                            a(href="#").btn.btn--result.btn--select Select
                            .rule
                                .rule__left
                                    .rule__title Fare Rule
                                    .rule__box
                                        ul
                                            li * Please note that cancellation charges will be applicable on total ticket value
                                            li * Refundable amount is calculated based on scheduled bus departure time from the first boarding time.
                                        ul
                                            li * Partial cancellation is not allowed for this ticket. Ticket cannot be cancelled after bus departure time
                                            li * Ticket cannot be cancelled after scehduled bus departure time from the first boarding point.
                                .rule__right
                                    ul
                                        li
                                            span.icon.icon--ac
                                            a(href="#") AC
                                        li
                                            span.icon.icon--wifi
                                            a(href="#") FREE WIFI
                                        li
                                            span.icon.icon--tivi
                                            a(href="#") TELEVISION
                                        li
                                            span.icon.icon--sandwich
                                            a(href="#") SNACKS
                            .result__box
                                .result__item.result__img
                                    img(src="img/bus3.png", alt="")
                                .result__item.result__time
                                    .result__row
                                        p.row--title Departure
                                        p.row--title Arrival
                                    .result__row
                                        span.row--address.row--padding LAG
                                        span.row--address.row--flex
                                            span.icon.icon--arrival
                                            span.row--mini-time 1hr 20 mins
                                        span.row--address.row--padding EDO
                                    .result__row
                                        p.row--time.row--padding 16:00
                                        p.row--time.row--padding 09:45
                                    .result__row
                                        p.row--date.row--padding 12 Jan
                                        p.row--date.row--padding 13 Jan
                                .result__item.result__info
                                    ul.result__service
                                        li
                                            a(href="#") 14 Seaters Arrival
                                        li
                                            a(href="#") 4.3 Rating
                                        li
                                            a(href="#") Fare Rule
                                        li
                                            a(href="#") Amenities
                                .result__item.result__price
                                    .result__price-box
                                        p.result__price-title N80,000
                                        .result__button
                                            a(href="#").btn.btn--result.btn--details Details
                                            a(href="#").btn.btn--result.btn--select Select
                            .result__box
                                .result__item.result__img
                                    img(src="img/bus4.png", alt="")
                                .result__item.result__time
                                    .result__row
                                        p.row--title Departure
                                        p.row--title Arrival
                                    .result__row
                                        span.row--address.row--padding LAG
                                        span.row--address.row--flex
                                            span.icon.icon--arrival
                                            span.row--mini-time 1hr 20 mins
                                        span.row--address.row--padding EDO
                                    .result__row
                                        p.row--time.row--padding 16:00
                                        p.row--time.row--padding 09:45
                                    .result__row
                                        p.row--date.row--padding 12 Jan
                                        p.row--date.row--padding 13 Jan
                                .result__item.result__info
                                    ul.result__service
                                        li
                                            a(href="#") 14 Seaters Arrival
                                        li
                                            a(href="#") 4.3 Rating
                                        li
                                            a(href="#") Fare Rule
                                        li
                                            a(href="#") Amenities
                                .result__item.result__price
                                    .result__price-box
                                        p.result__price-title N80,000
                                        .result__button
                                            a(href="#").btn.btn--result.btn--details Details
                                            a(href="#").btn.btn--result.btn--select Select
            .row
                footer.footer.container
                    .footer__list
                        .footer__item
                            .footer__title
                                h2 MENU LISTS
                            ul.footer__box
                                li
                                    a(href="javascript:void(0)") About us
                                li
                                    a(href="javascript:void(0)") Terms and Conditions
                                li
                                    a(href="javascript:void(0)") Cargo
                                li
                                    a(href="javascript:void(0)") FAQ’S
                                li
                                    a(href="javascript:void(0)") Charter Service
                        .footer__item
                            .footer__title
                                h2 CONTACT US
                            .footer__box
                                p Get through to us via our social media platform below. Instagram , Twitter and Facebook.
                                ul.social
                                    li.social__img
                                        a
                                            img(src="img/icon-ins.png", alt="")
                                    li.social__img
                                        a
                                            img(src="img/icon-twitter.png", alt="")
                                    li.social__img
                                        a
                                            img(src="img/icon-facebook.png", alt="")
                                p We have an online customer care support that can attend to all your complaints through our live chat and you can also call us on.
                                .contact
                                    .contact__img
                                        img(src="img/icon-phone.png", alt="")
                                    p 09076555353
                        .footer__item
                            .footer__title
                                h2 SEND A MESSAGE
                            .footer__box
                                label(for="email") Email Address
                                input(type="email" placeholder="Enter Email Address")#email
                                label(for="message") Message
                                input(type="text" placeholder="Type Message Here")#message
                                button#submit Send Message
